<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="res_company_view_form" model="ir.ui.view">
            <field name="name">res.company.form.stock.receive.list</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page name="page_stock_receive_list" string="Receive List">
                        <group name="grp_stock_receive_list_1" colspan="4" col="2" string="Allowed Product Categories">
                            <field name="allowed_receive_product_categ_ids" col="2" nolabel="1"/>
                        </group>
                        <group name="grp_stock_receive_list_2" colspan="4" col="2" string="Allowed Product">
                            <field name="allowed_receive_product_ids" col="2" nolabel="1"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
