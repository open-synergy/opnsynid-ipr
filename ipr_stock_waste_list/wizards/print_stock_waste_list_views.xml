<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<record id="print_stock_waste_list_view_form" model="ir.ui.view">
    <field name="name">stock.print.waste.list.form</field>
    <field name="model">stock.print_waste_list</field>
    <field name="inherit_id" ref="ipr_stock_list_common.stock_print_list_common_view_form"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='product_ids']" position="before">
            <field name="company_id" invisible="1"/>
            <field name="allowed_product_ids" widget="many2many_tags" invisible="1"/>
            <field name="allowed_product_categ_ids" widget="many2many_tags" invisible="1"/>
        </xpath>
        <xpath expr="//field[@name='product_ids']" position="attributes">
            <attribute name="domain">
                ['|',('id','in',allowed_product_ids[0][2]),('categ_id','in',allowed_product_categ_ids[0][2])]
            </attribute>
        </xpath>
    </field>
</record>

<record id="print_stock_waste_list_action" model="ir.actions.act_window">
    <field name="name">Print waste List</field>
    <field name="res_model">stock.print_waste_list</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="view_id" ref="print_stock_waste_list_view_form"/>
    <field name="target">new</field>
</record>

<menuitem
    id="print_stock_waste_list_menu"
    name="Waste List"
    parent="stock.next_id_61"
    action="print_stock_waste_list_action"
    sequence="1000"
/>

</data>
</openerp>
