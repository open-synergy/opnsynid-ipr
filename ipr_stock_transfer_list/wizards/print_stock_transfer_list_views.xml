<openerp>
<data>

<record id="print_stock_transfer_list_view_form" model="ir.ui.view">
    <field name="name">stock.print.transfer.list.form</field>
    <field name="model">stock.print_transfer_list</field>
    <field name="arch" type="xml">
        <form string="Print Stock Transfer List">
            <group name="group_1" colspan="4" col="2">
                <field name="company_id"/>
                <field name="date_start"/>
                <field name="date_end"/>
                <separator string="Warehouse(s)" colspan="2"/>
                <field name="warehouse_ids" nolabel="1" colspan="2"/>
                <field name="output_format"/>
            </group>
            <footer>
                <button name="action_print"
                        type="object"
                        string="Print"
                        class="oe_highlight"/>
                or
                <button special="cancel"
                        string="Cancel"/>
            </footer>
        </form>
    </field>
</record>

<record id="print_stock_transfer_list_action" model="ir.actions.act_window">
    <field name="name">Print Transfer List</field>
    <field name="res_model">stock.print_transfer_list</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
</record>

<menuitem
    id="print_stock_transfer_list_menu"
    name="Transfer List"
    parent="stock.next_id_61"
    action="print_stock_transfer_list_action"
    sequence="1000"
/>

</data>
</openerp>
