<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="stock_warehouse_view_form">
            <field name="name">Warehouse - Accurate Information</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="mode">extension</field>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//notebook" position="inside">
                        <page string="Accurate Information">
                            <group name="accurate_information_grp">
                                <field name="accurate_dept_id"/>
                                <field name="accurate_warehouse_id"/>
                                <field name="accurate_customer_id"/>
                                <field name="accurate_kode_warehouse"/>
                                <field name="accurate_ar_account"/>
                            </group>
                        </page>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</openerp>

