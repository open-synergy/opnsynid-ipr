<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="stock_move_transfer_in_list_view_tree" model="ir.ui.view">
            <field name="name">stock.move.transfer.in.list.tree</field>
            <field name="model">stock.move_transfer_in_list</field>
            <field name="inherit_id" ref="ipr_stock_list_common.stock_move_list_common_view_tree"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='product_uom_id']" position="after">
                      <field name="source_loc_wh_id"/>
                    </xpath>
                    <xpath expr="//field[@name='date']" position="before">
                      <field name="source_dest_loc_wh_id"/>
                    </xpath>
                </data>
            </field>
        </record>

        <record id="stock_move_transfer_in_list_view_search" model="ir.ui.view">
            <field name="name">stock.move.transfer.in.list.search</field>
            <field name="model">stock.move_transfer_in_list</field>
            <field name="inherit_id" ref="ipr_stock_list_common.stock_move_list_common_view_search"/>
            <field name="arch" type="xml">
                <data>
                </data>
            </field>
        </record>

        <record id="stock_move_transfer_in_list_action" model="ir.actions.act_window">
            <field name="name">Stock Transfer In List</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.move_transfer_in_list</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="stock_move_transfer_in_list_view_tree"/>
        </record>

    </data>
</openerp>
